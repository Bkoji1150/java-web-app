pipeline {
  agent { label 'docker' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
       sh 'terraform -version'
       sh 'sudo bash /root/.bash_profile'
       sh 'sudo mvn --version /home/agent/jenkinsagent/workspace/'
      }
    }
  }
}
