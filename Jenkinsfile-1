pipeline {
  agent { label 'docker' }
  tools {
        maven 'mvn'
        jdk   'jdk'
    }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
 stages {
       stage ('Cloning git & Build') {
          steps {
                checkout scm
            }
        }
         stage("Static Code analysis With sonar") {                                          
            steps {
              withSonarQubeEnv(installationName: 'sonar') {
                sh  'mvn clean sonar:sonar'
              }
            }
          }
}
}
