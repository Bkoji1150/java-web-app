pipeline {
  agent { label 'docker' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sonar-server') { 
        sh 'mvn --version && terraform --version'
         sh 'mvn -U clean install sonar:sonar'
        }
      }
    }
  }
}
